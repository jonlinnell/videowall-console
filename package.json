{
  "name": "videowall-console",
  "version": "0.1.0",
  "author": "Jon Linnell",
  "private": true,
  "dependencies": {
    "antd": "^3.10.9",
    "axios": "^0.18.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "2.1.1",
    "styled-components": "^4.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .jsx --ext .js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/prop-types": "warn",
      "semi": [
        2,
        "never"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-airbnb": "^17.1.0"
  }
}
